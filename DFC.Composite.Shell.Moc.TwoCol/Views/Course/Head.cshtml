@model HeadViewModel

@if (!string.IsNullOrWhiteSpace(Model.Title))
{
    ViewData["Title"] = $"Course | {Model.Title} | National Careers Service";

    <title>@ViewData["Title"]</title>
}

@if (Model.Contents != null)
{
    @Html.Raw(Model.Contents)
}

<p class="govuk-body">VisitsBody @Model.VisitsBody</p>
<p class="govuk-body">VisitsFooter @Model.VisitsFooter</p>